<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itsdonespeedquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ItsDoneSpeedQuiz">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuizsettingActivity"
            android:label="설정"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".QuizwordActivity"
            android:label="QuizwordActivity"
            android:screenOrientation="landscape"
          >
        </activity>
        <activity
            android:name=".QuizfinishActivity"
            android:label="QuizfinishActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name=".QuizrecordActivity"
            android:label="기록"
            android:screenOrientation="portrait">
        </activity>
        <service android:name=".Appservice.MusicService"
            android:enabled="true"/>
    </application>

</manifest>